rder(value);\n  }\n \n  \n\n  const filteredData = data\n    .sort((a,b) =>{\n          \n          \n          if(order === 'byname'){\n            let textA = a.name;\n            let textB = b.name;\n            return (textA<textB) ? -1 : (textA>textB)? 1 :0; \n        \n        }else if(order === 'default'){\n          \n          return  data2;\n        }\n      })\n\n    .filter((character)=>{\n    \n        return character.name.toLocaleLowerCase().includes(inputSearch.toLocaleLowerCase()) ;\n      \n    })\n    .filter((character)=>{\n      if(searchSpecies=== 'all'){\n        return data;\n      }else{\n        return character.species === searchSpecies;\n      }\n    })\n    console.log(data);\n    console.log(data2);\n    \n    \n  \n  \n   \n\n\n\n  return (\n    <div className=\"page\">\n    \n    <Header />\n    <main>\n      <Switch>\n    \n      <Route exact path='/'>\n        <section>\n          <Filter handleSearch={handleSearch} value={inputSearch} handleSearchSpecies={handleSearchSpecies} searchSpecies={searchSpecies} handleOrder={handleOrder} \n          order={order}/>\n          <CharacterList data={filteredData}  inputSearch= {inputSearch} />\n        </section>\n       </Route>\n       <Route path='/character/:id'>\n          \n          <CharacterDetail selectedCharacter={selectedCharacter} />\n          \n       </Route>\n\n       <Route>\n        <ReturnButton /> \n        <h2>Página no encontrada.</h2>\n       </Route>\n   \n    </Switch>\n    </main>\n\n  </div>\n  );\n}\n\nexport default App;\n","/Users/macbookpro/Desktop/Adalab/modulo-3-evaluacion-final-mariamolinagarcia/src/components/Header.js",[],"/Users/macbookpro/Desktop/Adalab/modulo-3-evaluacion-final-mariamolinagarcia/src/components/CharacterList.js",[],"/Users/macbookpro/Desktop/Adalab/modulo-3-evaluacion-final-mariamolinagarcia/src/components/CharacterDetail.js",[],"/Users/macbookpro/Desktop/Adalab/modulo-3-evaluacion-final-mariamolinagarcia/src/components/Filter.js",[],"/